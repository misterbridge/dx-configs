{
    "compilerOptions": {
        "target": "es6",
        "lib": ["dom", "esnext"],
        "jsx": "react",
        "noEmit": true,
        "isolatedModules": true,
        "strict": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "useUnknownInCatchVariables": false
    }
}
